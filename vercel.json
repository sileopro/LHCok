{
  "version": 2,
  "builds": [
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    },
    {
      "src": "requirements.txt",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/cron",
      "dest": "/api/cron.js"
    }
  ],
  "functions": {
    "api/cron.js": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
} 
