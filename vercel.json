{
  "version": 2,
  "builds": [
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/cron",
      "dest": "/api/cron.js",
      "methods": ["GET", "POST"]
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "*/5 * * * *"
    }
  ]
} 