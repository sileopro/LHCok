{
  "version": 2,
  "builds": [
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    },
    {
      "src": "requirements.txt",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/cron",
      "dest": "/api/cron.js"
    }
  ]
} 
