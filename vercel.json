{
  "version": 2,
  "builds": [
    {
      "src": "api/run.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/run.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "PYTHONUNBUFFERED": "1"
  }
} 