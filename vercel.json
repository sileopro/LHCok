{
  "version": 2,
  "builds": [
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/run.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/scraper.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/run",
      "dest": "api/run.py"
    },
    {
      "src": "/api/cron",
      "dest": "api/cron.js"
    }
  ]
} 